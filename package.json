    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.9.1",
    "expect": "^21.0.2",
    "jsdom": "^11.2.0",
